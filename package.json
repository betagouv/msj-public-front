{
  "name": "msj-public-front",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.13.1",
    "npm": "10.8.0"
  },
  "dependencies": {
    "@dataesr/react-dsfr": "^3.10.0",
    "@gouvfr/dsfr": "1.9.1",
    "@sentry/cli": "^2.39.1",
    "@sentry/react": "^8.45.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "font-awesome": "^4.7.0",
    "helmet": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "react-scripts": "5.0.1",
    "remixicon": "^4.5.0",
    "typescript": "^5.7.2",
    "uuid": "^10.0.0",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "node server.js",
    "api": "json-server --watch db.json --port 5001",
    "dev": "react-scripts start",
    "run:app": "start-server-and-test api http://localhost:5001 dev",
    "build": "react-scripts build",
    "lint": "eslint . --ext .tsx,.ts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .tsx,.ts --ignore-path .gitignore --fix",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "eject": "react-scripts eject",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org betagouv --project msj-public-front ./build && sentry-cli --url https://sentry.incubateur.net sourcemaps upload --org betagouv --project msj-public-front ./build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "json-server": "^0.17.4",
    "postcss": "^8.4.49",
    "start-server-and-test": "^2.0.9",
    "tailwindcss": "^3.4.16",
    "webpack": "^5.97.1"
  },
  "overrides": {
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "remark-parse": {
      "trim": "0.0.3"
    },
    "css-select": {
      "nth-check": "2.1.1"
    }
  }
}

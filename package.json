{
  "name": "msj-public-front",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.13.1",
    "npm": "10.8.0"
  },
  "dependencies": {
    "@dataesr/react-dsfr": "^3.10.0",
    "@gouvfr/dsfr": "1.9.1",
    "@jonkoops/matomo-tracker-react": "^0.7.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.7",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "font-awesome": "^4.7.0",
    "helmet": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "remixicon": "^4.2.0",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "web-vitals": "^4.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "api": "json-server --watch db.json --port 5001",
    "dev": "react-scripts start",
    "run:app": "start-server-and-test api http://localhost:5001 dev",
    "build": "react-scripts build",
    "lint": "eslint . --ext .tsx,.ts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .tsx,.ts --ignore-path .gitignore --fix",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/builder-webpack5": "^8.1.3",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^8.1.1",
    "@storybook/preset-create-react-app": "^8.1.1",
    "@storybook/react": "^8.1.3",
    "@storybook/testing-library": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.2.25",
    "autoprefixer": "^10.4.19",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "json-server": "^0.17.4",
    "postcss": "^8.4.35",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.3",
    "webpack": "^5.91.0"
  },
  "overrides": {
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "postcss": "^8.4.35",
    "remark-parse": {
      "trim": "0.0.3"
    },
    "css-select": {
      "nth-check": "2.1.1"
    }
  }
}

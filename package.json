{
  "name": "msj-public-front",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.13.1",
    "npm": "10.8.0"
  },
  "dependencies": {
    "@dataesr/react-dsfr": "^3.10.0",
    "@gouvfr/dsfr": "1.9.1",
    "@sentry/cli": "^2.33.1",
    "@sentry/react": "^8.26.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "axios": "^1.7.5",
    "classnames": "^2.5.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "font-awesome": "^4.7.0",
    "helmet": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "remixicon": "^4.3.0",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0",
    "web-vitals": "^4.2.3"
  },
  "scripts": {
    "start": "node server.js",
    "api": "json-server --watch db.json --port 5001",
    "dev": "react-scripts start",
    "run:app": "start-server-and-test api http://localhost:5001 dev",
    "build": "react-scripts build",
    "lint": "eslint . --ext .tsx,.ts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .tsx,.ts --ignore-path .gitignore --fix",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "eject": "react-scripts eject",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org betagouv --project msj-public-front ./build && sentry-cli --url https://sentry.incubateur.net sourcemaps upload --org betagouv --project msj-public-front ./build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^8.1.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.7.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "json-server": "^0.17.4",
    "postcss": "^8.4.35",
    "start-server-and-test": "^2.0.5",
    "tailwindcss": "^3.4.10",
    "webpack": "^5.94.0"
  },
  "overrides": {
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "postcss": "^8.4.35",
    "remark-parse": {
      "trim": "0.0.3"
    },
    "css-select": {
      "nth-check": "2.1.1"
    }
  }
}
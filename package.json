{
  "name": "msj-public-front",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "dependencies": {
    "@dataesr/react-dsfr": "^3.1.7",
    "@gouvfr/dsfr": "^1.7.0",
    "@jonkoops/matomo-tracker-react": "^0.7.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "axios": "^1.2.2",
    "classnames": "^2.3.1",
    "express": "^4.18.1",
    "font-awesome": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "remixicon": "^2.5.0",
    "typescript": "^4.8.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "node server.js",
    "api": "json-server --watch db.json --port 5001",
    "dev": "react-scripts start",
    "run:app": "start-server-and-test api http://localhost:5001 dev",
    "build": "react-scripts build",
    "lint": "eslint . --ext .tsx,.ts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .tsx,.ts --ignore-path .gitignore --fix",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.6.8",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "autoprefixer": "^10.4.12",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.6",
    "jest": "^29.3.1",
    "json-server": "^0.17.0",
    "postcss": "^8.4.31",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "webpack": "^5.88.0"
  },
  "overrides": {
    "remark-parse": {
      "trim": "0.0.3"
    },
    "css-select": {
      "nth-check": "2.1.1"
    }
  }
}
